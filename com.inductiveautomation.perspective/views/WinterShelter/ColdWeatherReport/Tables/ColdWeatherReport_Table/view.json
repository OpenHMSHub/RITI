            {
              "messageType": "tableToExcel",
              "pageScope": true,
              "script": "\t# implement your handler here\n\t\n\timport traceback\n\ttry:\n\t\t\n\t\tfilename \u003d \u0027WinterShelter_ColdWeatherReport_\u0027 + str(system.date.format(system.date.now(),\"yyyyMMdd\")) + \u0027.xls\u0027\n\t\tdataset \u003d self.custom.sqlData\n\t\tNewDataSet \u003d []\n\t\theaders \u003d [\"Day of the Week\",\"Date\",\"Overflow Shelter\",\"GH Beds\", \"GH Utilization\",\"Congregational Beds\", \"Congregational Bed Utilization\", \"Total Beds Capacity at RITI\", \"Total Beds Utilized at RITI\", \"Added Beds\", \"Unutilized Beds\",\"Notes\"]\n\t\tfor i in range(dataset.getRowCount()):\n\t\t\tmonthDate \u003d dataset.getValueAt(i,\u0027monthDate\u0027)\n\t\t\tif monthDate is not None:\n\t\t\t\tmonthDate \u003d system.date.format(monthDate, \u0027MM/dd/yyyy\u0027)\n\t\t\t\n\t\t\t\n\t\t\tNewDataSet.append([dataset.getValueAt(i,\u0027dayOfWeek\u0027),monthDate, dataset.getValueAt(i,\u0027overflowShelter\u0027), dataset.getValueAt(i,\u0027ghBeds\u0027),dataset.getValueAt(i,\u0027ghUtilization\u0027),dataset.getValueAt(i,\u0027congregationalBeds\u0027),dataset.getValueAt(i,\u0027congregationalBedsUtilization\u0027),dataset.getValueAt(i,\u0027totalBedsCapacityRITI\u0027),dataset.getValueAt(i,\u0027totalBedsUtilizedRITI\u0027),dataset.getValueAt(i,\u0027addedBeds\u0027),dataset.getValueAt(i,\u0027unUtilizedBeds\u0027),dataset.getValueAt(i,\u0027notes\u0027)])\t\n\t\t\n\t\t## add \tsummary row\n#\t\tNewDataSet.append([\u0027\u0027,\u0027\u0027,None,None,None, None, None, self.getSibling(\"Summary\").custom.totalBedsUtilizedAtRiti, self.getSibling(\"Summary\").custom.addedBeds, self.getSibling(\"Summary\").custom.unUtilizedBeds,\u0027\u0027])\n\t\tNewDataSet \u003d system.dataset.toDataSet(headers, NewDataSet)\n\n\t\txlFile \u003d system.dataset.toExcel(True,NewDataSet)\n\t\tsystem.perspective.download(filename, xlFile)\n\texcept:   \n\t\tlogger \u003d system.util.getLogger(\"Wintershelter ColdWeatherReport tableToExcel\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)",
              "sessionScope": true,
              "viewScope": true
            },
            {
              "messageType": "ColdWeatherDay_updated",
              "pageScope": true,
              "script": "\t# implement your handler here\n\tself.refreshBinding(\"custom.sqlData\")",
              "sessionScope": false,
              "viewScope": false
            },
            {
              "messageType": "tableToPDF",
              "pageScope": true,
              "script": "\timport traceback\n\ttry:\n\t\t#Generate the report\n\t\tisColdValue \u003d self.session.custom.filters.coldWeatherReport.isCold.value\n\t\t\n\t\tisCold \u003d 0\n\t\tif isColdValue \u003d\u003d 1:\n\t\t\tisCold \u003d 1\n\t\t\n\t\t## dateRange for display - derive from query string monthDate BETWEEN \u00272023-11-01\u0027 AND \u00272023-11-26\u0027\n\t\tdateRangeValue \u003d self.custom.dateRangeValue\n\t\tdatesArr \u003d dateRangeValue.replace(\"monthDate BETWEEN \", \"\").split(\"AND\")\n\t\tstartDateStr \u003d datesArr[0].replace(\"\u0027\",\"\").strip()\n\t\tendDateStr \u003d datesArr[1].replace(\"\u0027\",\"\").strip()\n\t\t\n\t\tstartDateReqd \u003d system.date.format(system.date.parse(startDateStr, \"yyyy-MM-dd\"), \u0027MM/dd/yy\u0027)\n\t\tendDateReqd \u003d  system.date.format(system.date.parse(endDateStr, \"yyyy-MM-dd\"), \u0027MM/dd/yy\u0027)\n\n\t\tDateRangeForDisplay \u003d startDateReqd + \" - \" + endDateReqd\n\t\tproject \u003d \"HMS\"\n\t\tpath \u003d \"WinterShelter/ColdWeatherReport\"\n\t\tparameters \u003d {\u0027DateRangeForQuery\u0027:self.custom.dateRangeValue, \n\t\t\u0027isCold\u0027:isCold, \n\t\t\u0027DateRangeForDisplay\u0027:DateRangeForDisplay,\n\t\t\u0027TotalUtilizedBedsRITI\u0027:self.getSibling(\"Summary\").custom.totalBedsUtilizedAtRiti,\n\t\t\u0027AddedBedsByRITI\u0027:self.getSibling(\"Summary\").custom.addedBeds, \n\t\t\u0027NotUtilizedBeds\u0027:self.getSibling(\"Summary\").custom.unUtilizedBeds}\n#\t\tsystem.perspective.print(parameters)\n\t\tpdfFile \u003d system.report.executeReport(path,project,parameters,fileType\u003d\u0027pdf\u0027)\n\t\tfilename \u003d \u0027Wintershelter_ColdWeatherReport_\u0027 + str(system.date.format(system.date.now(),\"yyyyMMddHHmmss\")) + \u0027.pdf\u0027\n\t\t\n\t\tsystem.perspective.download(filename, pdfFile)\n\texcept:   \n\t\tlogger \u003d system.util.getLogger(\"Wintershelter ColdWeatherReport tableToPDF\")\n\t\tmsg \u003d traceback.format_exc()\n\t\tlogger.error(msg)",
              "sessionScope": true,
              "viewScope": true
            }
