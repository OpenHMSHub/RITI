        "propConfig": {
          "custom.dateRangeValue": {
            "binding": {
              "config": {
                "path": "session.custom.filters.coldWeatherReport.monthDate.where"
              },
              "transforms": [
                {
                  "code": "\tif value \u003d\u003d \u00271\u003d1\u0027: ## in case of no range , show from month to date\n\t\tendDate \u003d system.date.now()\n\t\tmonth \u003d system.date.getMonth(endDate)\n\t\tyear \u003d system.date.getYear(endDate)\n\t\tstartDate \u003d system.date.getDate(year, month, 1)\n\t\tqueryStr \u003d \"monthDate BETWEEN \u0027\" + system.date.format(startDate,\u0027yyyy-MM-dd\u0027) + \"\u0027 AND \u0027\" + system.date.format(endDate,\u0027yyyy-MM-dd\u0027) + \"\u0027\"\n\t\treturn queryStr\n\telse:\n\t\treturn value",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "custom.sqlData": {
            "binding": {
              "config": {
                "parameters": {
                  "dateRange": "{this.custom.dateRangeValue}",
                  "queryColdOnly": "if({session.custom.filters.coldWeatherReport.isCold.value} \u003d 1, 1, 0)"
                },
                "queryPath": "WinterShelter/ColdWeatherReport/GetReportData",
                "returnFormat": "dataset"
              },
              "type": "query"
            }
          },
          "custom.tableData": {
            "binding": {
              "config": {
                "path": "this.custom.sqlData"
              },
              "transforms": [
                {
                  "code": "\tFinalList \u003d []\n\tdataset \u003d value\n\tsumBedsUtilizedRITI \u003d 0\n\tsumAddedBeds \u003d 0\n\tsumUnutilizedBeds \u003d 0\n\tfor i in range(0,dataset.getRowCount()):\n\t\trow \u003d {}\n\t\tisCold \u003d bool(dataset.getValueAt(i,\u0027isCold\u0027))\n\t\trow[\u0027id\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027id\u0027), \u0027style\u0027: {}}\n\t\tif isCold:\n\t\t\trow[\u0027dayOfWeek\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027dayOfWeek\u0027), \u0027style\u0027: {\u0027background\u0027:\u0027#A5D4EA\u0027}}\n\t\t\trow[\u0027monthDate\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027monthDate\u0027), \u0027style\u0027: {\u0027background\u0027:\u0027#A5D4EA\u0027}}\n\t\t\trow[\u0027overflowShelter\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027overflowShelter\u0027), \u0027style\u0027: {\u0027background\u0027:\u0027#A5D4EA\u0027, \u0027color\u0027:\u0027#0066FF\u0027}}\n\t\t\trow[\u0027ghBeds\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027ghBeds\u0027), \u0027style\u0027: {\u0027background\u0027:\u0027#A5D4EA\u0027}}\n\t\t\trow[\u0027ghUtilization\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027ghUtilization\u0027), \u0027style\u0027: {\u0027background\u0027:\u0027#A5D4EA\u0027}}\n\t\t\trow[\u0027congregationalBeds\u0027] \u003d{\u0027value\u0027:  dataset.getValueAt(i,\u0027congregationalBeds\u0027), \u0027style\u0027: {\u0027background\u0027:\u0027#A5D4EA\u0027}}\n\t\t\trow[\u0027congregationalBedsUtilization\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027congregationalBedsUtilization\u0027), \u0027style\u0027: {\u0027background\u0027:\u0027#A5D4EA\u0027}}\n\t\t\trow[\u0027totalBedsCapacityRITI\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027totalBedsCapacityRITI\u0027), \u0027style\u0027: {\u0027background\u0027:\u0027#A5D4EA\u0027}}\n\t\telse:\n\t\t\trow[\u0027dayOfWeek\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027dayOfWeek\u0027), \u0027style\u0027: {}}\n\t\t\trow[\u0027monthDate\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027monthDate\u0027), \u0027style\u0027: {}}\n\t\t\trow[\u0027overflowShelter\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027overflowShelter\u0027), \u0027style\u0027: {\u0027color\u0027:\u0027#0066FF\u0027}}\n\t\t\trow[\u0027ghBeds\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027ghBeds\u0027), \u0027style\u0027: {}}\n\t\t\trow[\u0027ghUtilization\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027ghUtilization\u0027), \u0027style\u0027: {}}\n\t\t\trow[\u0027congregationalBeds\u0027] \u003d{\u0027value\u0027:  dataset.getValueAt(i,\u0027congregationalBeds\u0027), \u0027style\u0027: {}}\n\t\t\trow[\u0027congregationalBedsUtilization\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027congregationalBedsUtilization\u0027), \u0027style\u0027: {}}\n\t\t\trow[\u0027totalBedsCapacityRITI\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027totalBedsCapacityRITI\u0027), \u0027style\u0027: {}}\n\t\trow[\u0027totalBedsUtilizedRITI\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027totalBedsUtilizedRITI\u0027), \u0027style\u0027: {}}\n\t\trow[\u0027addedBeds\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027addedBeds\u0027), \u0027style\u0027: {\u0027color\u0027:\u0027#1CBFBB\u0027}}\n\t\trow[\u0027unUtilizedBeds\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027unUtilizedBeds\u0027), \u0027style\u0027: {\u0027color\u0027:\u0027#FF7A59\u0027}}\n\t\trow[\u0027notes\u0027] \u003d {\u0027value\u0027: dataset.getValueAt(i,\u0027notes\u0027), \u0027style\u0027: {}}\n\t\tif dataset.getValueAt(i,\u0027totalBedsUtilizedRITI\u0027) is not None:\n\t\t\tsumBedsUtilizedRITI \u003d sumBedsUtilizedRITI + dataset.getValueAt(i,\u0027totalBedsUtilizedRITI\u0027)\n\t\t\n\t\tif dataset.getValueAt(i,\u0027addedBeds\u0027) is not None:\n\t\t\tsumAddedBeds \u003d sumAddedBeds + dataset.getValueAt(i,\u0027addedBeds\u0027)\n\t\t\t\n\t\tif dataset.getValueAt(i,\u0027unUtilizedBeds\u0027) is not None:\n\t\t\tsumUnutilizedBeds \u003d sumUnutilizedBeds + dataset.getValueAt(i,\u0027unUtilizedBeds\u0027)\n\t\t\n\t\tFinalList.append(row)\n\tself.getSibling(\"Summary\").custom.totalBedsUtilizedAtRiti \u003d sumBedsUtilizedRITI\n\tself.getSibling(\"Summary\").custom.addedBeds \u003d sumAddedBeds\n\tself.getSibling(\"Summary\").custom.unUtilizedBeds \u003d sumUnutilizedBeds\n\treturn FinalList",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
