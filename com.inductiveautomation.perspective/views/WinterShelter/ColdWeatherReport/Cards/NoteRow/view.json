{
  "custom": {},
  "params": {
    "Note": "value"
  },
  "propConfig": {
    "params.Note": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 30
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "30px",
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Note"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Detail_Action"
          },
          "textStyle": {
            "textAlign": "left",
            "textIndent": 10
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}