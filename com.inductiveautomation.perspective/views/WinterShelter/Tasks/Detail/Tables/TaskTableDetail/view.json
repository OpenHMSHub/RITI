            "onRowClick": {
              "config": {
                "script": "\t#04/30/21 Copied from Registrations table\n#\tselectedColumn \u003d self.props.selection.selectedColumn\n#\t#Display the detail screen\n#\tcongregationID \u003d self.props.data[event.row][\u0027id\u0027]\n#\tbackID \u003d 5\n#\tpage \u003d \u0027/WinterShelter/Congregations/Detail/\u0027 + str(congregationID) + \u0027/\u0027  + backID\n#\tsystem.perspective.navigate(page)\n\n\t#Show the selected task details at the top of the page.\n\tself.view.params.selectedRowData  \u003d self.props.data[event.row]\n\tself.session.custom.NewWinterShelterTask \u003d {\n\t  \"Task Type\": \"\",\n\t  \"Task\": None,\n\t  \"notes\": \"\",\n\t  \"Location Name\": \"\",\n\t  \"completed\": None,\n\t  \"priority\": \"\",\n\t  \"Created\": None,\n\t  \"priorityId\": -1,\n\t  \"taskTypeId\": -1,\n\t  \"statusId\": -1,\n\t  \"locationId\": -1,\n\t  \"contact\": \"\",\n\t  \"staffName\": \"\",\n\t  \"id\": -1,\n\t  \"staffId\": -1,\n\t  \"Due Date\": \"\",\n\t  \"status\": \"\"\n\t}"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Table"
        },
        "position": {
          "basis": "800px",
          "grow": 1
        },
        "propConfig": {
          "props.data": {
            "binding": {
              "config": {
                "path": "view.custom.table_data"
              },
              "transforms": [
                {
                  "code": "\ttable_data \u003d []\n\t#Iterate over the results of the query\n\tfor row in range(value.getRowCount()):\n\t\trow_data \u003d {}\n\t\tstyle \u003d {}\n\t\tstyle[\u0027classes\u0027] \u003d \"Table/Cell_Dashboard\"\n\t\t#Map query results into correct attributes\n\t\trow_data[\u0027id\u0027] \u003d value.getValueAt(row, \"id\")\n\t\t#row_data[\u0027options\u0027] \u003d \u0027\u0027\n\t\trow_data[\u0027Task Type\u0027] \u003d value.getValueAt(row, \"type\")\n\t\trow_data[\u0027Task\u0027] \u003d value.getValueAt(row, \"title\")\n\t\trow_data[\u0027Location Name\u0027] \u003d value.getValueAt(row, \"locationName\")\n\t\trow_data[\u0027Due Date\u0027] \u003d value.getValueAt(row, \"dueDate\")\n\t\trow_data[\u0027Created\u0027] \u003d system.date.format(value.getValueAt(row, \"timeCreated\"), \"MM/dd/YYYY hh:mm a\")\n\t\trow_data[\u0027notes\u0027] \u003d value.getValueAt(row, \"notes\")\n\t\trow_data[\u0027locationId\u0027] \u003d value.getValueAt(row, \"locationId\")\n\t\trow_data[\u0027taskTypeId\u0027] \u003d value.getValueAt(row, \"taskTypeId\")\n\t\trow_data[\u0027staffId\u0027] \u003d value.getValueAt(row, \"staffId\")\n\t\trow_data[\u0027statusId\u0027] \u003d value.getValueAt(row, \"statusId\")\n\t\trow_data[\u0027priority\u0027] \u003d value.getValueAt(row, \"taskPriority\")\n\t\trow_data[\u0027priorityId\u0027] \u003d value.getValueAt(row, \"priorityId\")\n\t\trow_data[\u0027staffName\u0027] \u003d value.getValueAt(row, \"staffName\")\n\t\trow_data[\u0027status\u0027] \u003d value.getValueAt(row, \"Status\")\n\t\trow_data[\u0027contact\u0027] \u003d value.getValueAt(row, \"contact\")\n\t\tif value.getValueAt(row, \"Completed\") is None:\n\t\t\trow_data[\u0027completed\u0027] \u003d \u0027\u0027\n\t\telse:\n\t\t\trow_data[\u0027completed\u0027] \u003d value.getValueAt(row, \"Completed\")\n\t\t#Append the individual row date to the table data object\n\t\ttable_data.append(row_data)\n\t\t\n\treturn table_data\n#\treturn value",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
