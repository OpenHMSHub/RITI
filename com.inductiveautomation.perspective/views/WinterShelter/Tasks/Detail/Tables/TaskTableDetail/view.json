  "propConfig": {
    "custom.filtered_data": {
      "binding": {
        "config": {
          "struct": {
            "filters": "{view.custom.filters}",
            "source": "hasChanged({view.custom.sql_data})"
          },
          "waitOnAll": true
        },
        "transforms": [
          {
            "code": "\tfiltered_data \u003d filters.applyFiltersOnDataset(self.custom.sql_data, self.custom.filters)\n\t\n\treturn filtered_data",
            "type": "script"
          }
        ],
        "type": "expr-struct"
      },
      "persistent": true
    },
    "custom.filters": {
      "binding": {
        "config": {
          "struct": {
            "congregationId": "{view.params.filter_congregationId}",
            "priorityId": "{view.params.filter_priorityId}",
            "searchText": "{view.params.filter_searchText}",
            "statusId": "{view.params.filter_statusId}"
          },
          "waitOnAll": true
        },
        "transforms": [
          {
            "code": "\tfilters \u003d []\n#\tif value[\u0027congregationId\u0027] is not None and value[\u0027congregationId\u0027] !\u003d \"\" and value[\u0027congregationId\u0027] !\u003d -1:\n#\t\toneFilter \u003d {}\n#\t\toneFilter[\u0027type\u0027] \u003d \"equals\"\n#\t\toneFilter[\u0027key\u0027] \u003d \"locationId\"\n#\t\toneFilter[\u0027value\u0027] \u003d value[\u0027congregationId\u0027]\n#\t\tfilters.append(oneFilter)\n#\t\n#\tif value[\u0027priorityId\u0027] is not None and value[\u0027priorityId\u0027] !\u003d \"\" and value[\u0027priorityId\u0027] !\u003d -1:\n#\t\toneFilter \u003d {}\n#\t\toneFilter[\u0027type\u0027] \u003d \"equals\"\n#\t\toneFilter[\u0027key\u0027] \u003d \"priorityId\"\n#\t\toneFilter[\u0027value\u0027] \u003d value[\u0027priorityId\u0027]\n#\t\tfilters.append(oneFilter)\n#\t\n#\tif value[\u0027statusId\u0027] is not None and value[\u0027statusId\u0027] !\u003d \"\" and value[\u0027statusId\u0027] !\u003d -1:\n#\t\toneFilter \u003d {}\n#\t\toneFilter[\u0027type\u0027] \u003d \"equals\"\n#\t\toneFilter[\u0027key\u0027] \u003d \"statusId\"\n#\t\toneFilter[\u0027value\u0027] \u003d value[\u0027statusId\u0027]\n#\t\tfilters.append(oneFilter)\n#\t\n#\tif value[\u0027searchText\u0027] is not None and value[\u0027searchText\u0027] !\u003d \"\" and value[\u0027searchText\u0027] !\u003d -1:\n#\t\toneFilter \u003d {}\n#\t\toneFilter[\u0027type\u0027] \u003d \"like\"\n#\t\toneFilter[\u0027key\u0027] \u003d \"\"\n#\t\toneFilter[\u0027value\u0027] \u003d value[\u0027searchText\u0027]\n#\t\tfilters.append(oneFilter)\n\treturn filters",
            "type": "script"
          }
        ],
        "type": "expr-struct"
      },
      "persistent": true
    },
    "custom.sql_data": {
      "binding": {
        "config": {
          "designerUseLimit": false,
          "parameters": {
            "activeInactive": "{view.params.filter_activeInactive}",
            "congregationId": "{view.params.filter_congregationId}",
            "dateRangeEnd": "{view.params.filter_dateRangeEnd}",
            "dateRangeStart": "{view.params.filter_dateRangeStart}",
            "priorityId": "{view.params.filter_priorityId}",
            "searchText": "CONCAT(\"%\",{view.params.filter_searchText},\"%\")",
            "staffId": "{view.params.filter_staffId}",
            "startDateFilter": "getDate(1970,1,1)",
            "statusId": "{view.params.filter_statusId}"
          },
          "queryPath": "WinterShelter/Tasks/Tasks"
        },
        "type": "query"
      },
      "persistent": true
    },
