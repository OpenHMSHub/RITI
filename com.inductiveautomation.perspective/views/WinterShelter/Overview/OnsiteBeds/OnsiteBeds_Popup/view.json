{
  "custom": {
    "selectedSeasonId": 14,
    "selectedSeasonString": "2024-2025",
    "selectedSeasonYear1": 2024,
    "selectedSeasonYear2": 2025
  },
  "events": {
    "system": {
      "onStartup": {
        "config": {
          "script": "\tself.session.custom.selectedServeDate \u003d system.date.now()\n\tself.session.custom.SelectedDayofMonth \u003d \u0027\u0027"
        },
        "scope": "G",
        "type": "script"
      }
    }
  },
  "params": {
    "seasonId": 14
  },
  "propConfig": {
    "custom.selectedSeasonId": {
      "binding": {
        "config": {
          "path": "view.params.seasonId"
        },
        "type": "property"
      },
      "persistent": true
    },
    "custom.selectedSeasonString": {
      "binding": {
        "config": {
          "queryPath": "WinterShelterGlobal/getCurrentSeason"
        },
        "transforms": [
          {
            "code": "\treturn value.getValueAt(0,\u0027seasons\u0027)",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "custom.selectedSeasonYear1": {
      "binding": {
        "config": {
          "expression": "{view.custom.selectedSeasonString}"
        },
        "transforms": [
          {
            "code": "\tif value is not None and value !\u003d \"\":\n\t\treturn int(value.split(\"-\")[0])\n\telse:\n\t\treturn \u0027\u0027\n\t\t",
            "type": "script"
          }
        ],
        "type": "expr"
      },
      "persistent": true
    },
    "custom.selectedSeasonYear2": {
      "binding": {
        "config": {
          "expression": "{view.custom.selectedSeasonString}"
        },
        "transforms": [
          {
            "code": "\tif value is not None and value !\u003d \"\":\n\t\treturn int(value.split(\"-\")[1])\n\telse:\n\t\treturn \u0027\u0027\n\t\t",
            "type": "script"
          }
        ],
        "type": "expr"
      },
      "persistent": true
    },
    "params.seasonId": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "width": 1020
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Header"
            },
            "position": {
              "basis": "320px",
              "grow": 1
            },
            "props": {
              "path": "WinterShelter/Overview/OnsiteBeds/Header"
            },
            "type": "ia.display.view"
          }
        ],
        "meta": {
          "name": "Row_Header"
        },
        "position": {
          "basis": "40px"
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "LeftFiller"
            },
            "position": {
              "basis": "40px"
            },
            "type": "ia.container.flex"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "ServeCalendar"
                },
                "position": {
                  "basis": "400px"
                },
                "propConfig": {
                  "props.params.seasonId": {
                    "binding": {
                      "config": {
                        "path": "view.custom.selectedSeasonId"
                      },
                      "type": "property"
                    }
                  },
                  "props.params.year1": {
                    "binding": {
                      "config": {
                        "path": "view.custom.selectedSeasonYear1"
                      },
                      "type": "property"
                    }
                  },
                  "props.params.year2": {
                    "binding": {
                      "config": {
                        "path": "view.custom.selectedSeasonYear1"
                      },
                      "type": "property"
                    }
                  }
                },
                "props": {
                  "params": {
                    "month1": 10,
                    "month2": 11
                  },
                  "path": "WinterShelter/Overview/OnsiteBeds/Calendar",
                  "style": {
                    "marginTop": 10
                  }
                },
                "type": "ia.display.view"
              },
              {
                "children": [
                  {
                    "meta": {
                      "name": "season"
                    },
                    "position": {
                      "basis": "140px",
                      "shrink": 0
                    },
                    "propConfig": {
                      "props.params.options": {
                        "binding": {
                          "config": {
                            "queryPath": "WinterShelterGlobal/Filters/getSeasons"
                          },
                          "type": "query"
                        }
                      },
                      "props.params.placeholder": {
                        "binding": {
                          "config": {
                            "expression": "toInt(if (getMonth(now()),getYear(now()),getYear(now())-1))"
                          },
                          "transforms": [
                            {
                              "code": "\treturn str(value) + \u0027-\u0027 + str(value+1)",
                              "type": "script"
                            }
                          ],
                          "type": "expr"
                        }
                      },
                      "props.params.selection": {
                        "binding": {
                          "config": {
                            "queryPath": "WinterShelterGlobal/getCurrentSeason"
                          },
                          "transforms": [
                            {
                              "code": "\treturn value.getValueAt(0,\u0027id\u0027)",
                              "type": "script"
                            }
                          ],
                          "type": "query"
                        },
                        "onChange": {
                          "enabled": null,
                          "script": "\tsystem.perspective.sendMessage(\"clearScheduleSelection\", payload \u003d {}, scope \u003d \u0027session\u0027)"
                        }
                      }
                    },
                    "props": {
                      "path": "1_globalTEMPLATES/Filtering/Conditionals/Conditional_SingleDropDown_NoClear",
                      "style": {
                        "classes": "Filter/Filter_Container"
                      }
                    },
                    "type": "ia.display.view"
                  }
                ],
                "meta": {
                  "name": "Filter",
                  "visible": false
                },
                "position": {
                  "basis": "30px"
                },
                "type": "ia.container.flex"
              },
              {
                "meta": {
                  "name": "Detail_Schedule_Table"
                },
                "position": {
                  "basis": "320px",
                  "grow": 1
                },
                "propConfig": {
                  "props.params.seasonId": {
                    "binding": {
                      "config": {
                        "path": "../Filter/season.props.params.selection"
                      },
                      "type": "property"
                    }
                  },
                  "props.params.year1": {
                    "binding": {
                      "config": {
                        "path": "view.custom.selectedSeasonYear1"
                      },
                      "type": "property"
                    }
                  },
                  "props.params.year2": {
                    "binding": {
                      "config": {
                        "path": "view.custom.selectedSeasonYear2"
                      },
                      "type": "property"
                    }
                  }
                },
                "props": {
                  "path": "WinterShelter/Overview/OnsiteBeds/Table"
                },
                "type": "ia.display.view"
              }
            ],
            "meta": {
              "name": "LeftColumn"
            },
            "position": {
              "basis": "300px",
              "grow": 1
            },
            "props": {
              "direction": "column"
            },
            "type": "ia.container.flex"
          },
          {
            "meta": {
              "name": "MiddleFiller"
            },
            "position": {
              "basis": "40px"
            },
            "type": "ia.container.flex"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "Summary"
                },
                "position": {
                  "basis": "780px"
                },
                "propConfig": {
                  "props.params.locationID": {
                    "binding": {
                      "config": {
                        "path": "view.params.locationId"
                      },
                      "type": "property"
                    }
                  },
                  "props.params.seasonEndYear": {
                    "binding": {
                      "config": {
                        "path": "view.custom.selectedSeasonYear2"
                      },
                      "type": "property"
                    }
                  },
                  "props.params.seasonId": {
                    "binding": {
                      "config": {
                        "path": "view.custom.selectedSeasonId"
                      },
                      "type": "property"
                    }
                  },
                  "props.params.seasonStartYear": {
                    "binding": {
                      "config": {
                        "path": "view.custom.selectedSeasonYear1"
                      },
                      "type": "property"
                    }
                  }
                },
                "props": {
                  "path": "WinterShelterGlobal/Schedule/Cards/CongregationSummarySchedule",
                  "style": {
                    "marginTop": 2,
                    "overflow": "hidden"
                  }
                },
                "type": "ia.display.view"
              },
              {
                "meta": {
                  "name": "MiddleFiller"
                },
                "position": {
                  "basis": "0px"
                },
                "type": "ia.container.flex"
              },
              {
                "meta": {
                  "name": "Summary_Detail_ParticipantsList"
                },
                "position": {
                  "basis": "220px",
                  "grow": 1
                },
                "props": {
                  "params": {
                    "selectedId": 0,
                    "selected_id": 0
                  },
                  "path": "WinterShelterGlobal/Schedule/Cards/Summary_Detail_ParticipantsList",
                  "style": {
                    "borderTopColor": "#668FB0",
                    "borderTopStyle": "solid",
                    "borderTopWidth": "1px"
                  }
                },
                "type": "ia.display.view"
              }
            ],
            "meta": {
              "name": "RightColumn"
            },
            "position": {
              "basis": "330px",
              "display": false
            },
            "props": {
              "direction": "column",
              "style": {
                "overflow": "hidden"
              }
            },
            "type": "ia.container.flex"
          }
        ],
        "meta": {
          "name": "Row_Content"
        },
        "position": {
          "basis": "200px",
          "grow": 1
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Background/Alternative",
        "marginTop": 0
      }
    },
    "type": "ia.container.flex"
  }
}