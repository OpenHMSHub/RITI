          "props.data": {
            "binding": {
              "config": {
                "parameters": {
                  "seasonId": "{view.params.seasonId}"
                },
                "queryPath": "WinterShelter/Overview/OnsiteBeds/Get_OnsiteBed_Details"
              },
              "transforms": [
                {
                  "code": "\tlogger \u003d system.util.getLogger(\"Schedule Detail\")\n\tinstances \u003d []\n\tstartDayOfSeason \u003d system.date.getDayOfYear(system.date.getDate(int(self.view.params.year1), 5, 1))\n\tendDayOfCurrentYear \u003d system.date.getDayOfYear(system.date.getDate(int(self.view.params.year1), 11, 31))\n\tendDayOfSeason \u003d system.date.getDayOfYear(system.date.getDate(self.view.params.year2, 2, 31))\n\tif value is not None and value.getRowCount() \u003e 0:\n#\t\t#check if any of the date has status added\n#\t\tparameters\u003d{\u0027locationId\u0027:self.view.params.locationId ,\u0027todayDayOfTheYear\u0027:self.view.custom.todayDayOfTheYear,\u0027seasonId\u0027:self.view.params.seasonId,\u0027status\u0027:\"status \u003d \u0027Addition\u0027\"}\n#\t\tresult \u003d system.db.runNamedQuery(\"WinterShelterGlobal/Schedule/GetScheduleCountPerStatus_ScheduleDetail\",parameters\u003dparameters)\n#\t\tIsAddedDates \u003d False\n#\t\tif result.getRowCount()\u003e0:\n#\t\t\tif result.getValueAt(0,0)\u003e0:\n#\t\t\t\tIsAddedDates \u003d True\n#\t\treturn IsAddedDates\n\t\tfor i in range(value.getRowCount()):\n\t\t\toneInstance \u003d {}\n\t\t\tseasonId \u003d value.getValueAt(i,\u0027seasonId\u0027)\n\t\t\tdayOfYear \u003d value.getValueAt(i,\u0027dayOfYear\u0027)\n\t\t\toneInstance[\u0027locationId\u0027] \u003d value.getValueAt(i,\u0027locationId\u0027)\n\t\t\toneInstance[\u0027scheduleId\u0027] \u003d value.getValueAt(i,\u0027scheduleId\u0027)\n\t\t\toneInstance[\u0027seasonId\u0027] \u003d seasonId\n\t\t\toneInstance[\u0027totalBeds\u0027] \u003d value.getValueAt(i,\u0027totalBeds\u0027)\n\t\t\toneInstance[\u0027gender\u0027] \u003d value.getValueAt(i,\u0027gender\u0027)\n\t\t\toneInstance[\u0027genderId\u0027] \u003d value.getValueAt(i,\u0027genderId\u0027)\n\t\t\toneInstance[\u0027dayOfYear\u0027] \u003d dayOfYear\n\t\t\toneInstance[\u0027status\u0027] \u003d value.getValueAt(i,\u0027status\u0027)\n\t\t\toneInstance[\u0027locationName\u0027] \u003d value.getValueAt(i,\u0027locationName\u0027)\n#\t\t\tif IsAddedDates:\n\t\t\tif value.getValueAt(i,\u0027status\u0027) \u003d\u003d \u0027Completed\u0027 or value.getValueAt(i,\u0027status\u0027) \u003d\u003d \u0027Scheduled\u0027:\n\t\t\t\toneInstance[\u0027status\u0027] \u003d {\u0027value\u0027:value.getValueAt(i,\u0027status\u0027),\u0027style\u0027:{\u0027color\u0027:\u0027#19F708\u0027}}\n\t\t\tif value.getValueAt(i,\u0027status\u0027) \u003d\u003d \u0027Canceled\u0027:\n\t\t\t\toneInstance[\u0027status\u0027] \u003d {\u0027value\u0027:value.getValueAt(i,\u0027status\u0027),\u0027style\u0027:{\u0027color\u0027:\u0027#E71D36\u0027}}\n\t\t\t\n\t\t\t## calclulate date and day based on day of year\n\t\t\t\n#\t\t\tif dayOfYear \u003e\u003d startDayOfSeason and dayOfYear \u003c\u003d endDayOfCurrentYear:\n#\t\t\t\tcalendarDate \u003d calendar.dayOfYearToDate(dayOfYear,self.view.params.year1)\n#\t\t\telse:\n#\t\t\t\tcalendarDate \u003d calendar.dayOfYearToDate(dayOfYear,self.view.params.year2)\n\t\t\tcalendarDate \u003d value.getValueAt(i,\u0027DateFromDayOfYear\u0027)\n\n\t\t\toneInstance[\u0027date\u0027] \u003d calendarDate\n\t\t\tday \u003d system.date.format(calendarDate, \u0027EEEE\u0027)\n\t\t\toneInstance[\u0027day\u0027] \u003d day \n\t\t\toneInstance[\u0027location\u0027] \u003d value.getValueAt(i,\u0027HostLocationType\u0027) if value.getValueAt(i,\u0027HostLocationType\u0027)!\u003dNone and value.getValueAt(i,\u0027HostLocationType\u0027)!\u003d\u0027\u0027 else self.view.custom.month1SeasonHostingType\n\t\t\t\n\t\t\tinstances.append(oneInstance)\n\tinstances.sort(key\u003dlambda x: x[\u0027date\u0027] or False, reverse\u003dFalse)\n#\tlogger.error(\"instances data from schedule detail table : \")\n#\tlogger.error(str(instances))\n\treturn instances\n\t",
                  "type": "script"
                }
              ],
              "type": "query"
            }
          },
