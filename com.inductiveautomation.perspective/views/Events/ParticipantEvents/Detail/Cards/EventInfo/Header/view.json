{
  "custom": {},
  "params": {
    "EventID": 4683,
    "Text": "Tyler George Adams"
  },
  "propConfig": {
    "params.EventID": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Text": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 170,
      "width": 250
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Image"
        },
        "position": {
          "basis": "120px",
          "shrink": 0
        },
        "propConfig": {
          "props.altText": {
            "binding": {
              "config": {
                "path": "view.params.Text"
              },
              "transforms": [
                {
                  "code": "\treturn value[0]",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.source": {
            "binding": {
              "config": {
                "path": "view.params.participantID"
              },
              "transforms": [
                {
                  "code": "\tfileName \u003d \"/system/images/people/{}.jpg\".format(value)\n\treturn fileName\n",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "fit": {
            "height": 120,
            "mode": "absolute",
            "width": 120
          },
          "style": {
            "classes": "Rounded_Image",
            "marginLeft": "auto",
            "marginRight": "auto",
            "marginTop": "10px"
          }
        },
        "type": "ia.display.image"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "32px"
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.Text"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Personal_Header",
            "textAlign": "center"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "borderBottomColor": "#E6E6E6",
        "borderBottomStyle": "solid",
        "borderBottomWidth": "0px",
        "classes": "Text/Personal_Header",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}