{
  "custom": {},
  "params": {
    "HeaderText": "value"
  },
  "propConfig": {
    "params.HeaderText": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 25
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "25px",
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.HeaderText"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Add_Header",
            "paddingBottom": 8,
            "paddingLeft": 8,
            "paddingTop": 8
          },
          "textStyle": {
            "color": "#ffffff",
            "fontFamily": "Proxima Nova ExCn",
            "fontSize": 20
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "background": "linear-gradient(90deg, rgba(22,165,189,1) 0%, rgba(13,185,165,1) 100%)"
      }
    },
    "type": "ia.container.flex"
  }
}