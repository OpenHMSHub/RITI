              "basis": "120px"
            },
            "props": {
              "style": {
                "classes": "Buttons/Secondary_Orange",
                "marginBottom": "15px",
                "marginLeft": "26px",
                "marginRight": "20px",
                "marginTop": "15px"
              },
              "text": "Clear"
            },
            "type": "ia.input.button"
          },
          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "script": "\n\ttodayDate \u003d system.date.now()\n\ttodayYear \u003d system.date.getYear(todayDate)\n\ttodayDay \u003d system.date.getDayOfMonth(todayDate)\n\ttodayMonth \u003d system.date.getMonth(todayDate)\n\ttodayMonth \u003d todayMonth + 1\n\tsystem.perspective.print(todayYear)\n\tsystem.perspective.print(todayDay)\n\tsystem.perspective.print(todayMonth)\n\tselectedEventId \u003d self.parent.parent.getChild(\"Table\").props.selection.data[0].id\n\tstartsOn \u003d self.parent.parent.getChild(\"Table\").props.selection.data[0].startsOn\n\tduration \u003d self.parent.parent.getChild(\"Table\").props.selection.data[0].duration_hours\n\tdescription \u003d self.parent.parent.getChild(\"Table\").props.selection.data[0].description\n\tpoints \u003d self.parent.parent.getChild(\"Table\").props.selection.data[0].points\n\tlocationId \u003d self.parent.parent.getChild(\"Table\").props.selection.data[0].locationId\n\t## check if this day is already in the schedule\n\tsystem.perspective.print(startsOn)\n\tstartsOn \u003d system.date.fromMillis(startsOn)\n\thour \u003d system.date.getHour24(startsOn)\n\tminute \u003d system.date.getMinute(startsOn)\n\tseconds \u003d system.date.getSecond(startsOn)\n\t\n\tnewStartsOn \u003d system.date.setTime(todayDate, hour, minute, seconds)\n\tnewEndsOn \u003d system.date.addHours(newStartsOn, duration)\n\tselectQ \u003d \"SELECT id from participant.EventSchedule WHERE eventId \u003d ? and day \u003d ? and month \u003d ? and year \u003d ?\"\n\texists \u003d system.db.runPrepQuery(selectQ, [selectedEventId, todayDay, todayMonth, todayYear])\n\tif exists is not None and exists.getRowCount() \u003e 0:\n\t\t## event already exists show error\n\t\tsystem.perspective.sendMessage(\"errorAddingEvent\", payload \u003d {\"message\":\"Event already added for current night\"}, scope\u003d\"session\")\n\telse:\n\t\t## add event to the schedule\n\t\tinsertQ \u003d \"\"\"INSERT INTO participant.EventSchedule \n\t\t(eventId,day,month,year, timeCreated,startsOn,endsOn, duration, locationId, points, description) \n\t\tVALUES (?,?,?,?,CURRENT_TIMESTAMP,?,?,?,?,?,?)\n\t\t\"\"\"\n\t\tsystem.db.runPrepUpdate(insertQ , [selectedEventId,  todayDay, todayMonth, todayYear,newStartsOn, newEndsOn, duration, locationId, points, description])\n\t\tsystem.perspective.sendMessage(\"refreshEvents\", payload \u003d {}, scope\u003d\"page\")\n\t\t\n\t\t## also check if event repeats is before today date, then update event repeats\n\t\tselectQ \u003d \"SELECT repeatDateUntil FROM participant.Events where id\u003d?\"\n\t\trepeatData \u003d system.db.runPrepQuery(selectQ, [selectedEventId])\n\t\tupdateRequired \u003d False\n\t\tif repeatData is not None and repeatData.getRowCount() \u003e 0:\n\t\t\t repeatDate \u003d repeatData.getValueAt(0,\u0027repeatDateUntil\u0027)\n\t\t\t system.perspective.print(repeatDate)\n\t\t\t if repeatDate is None or system.date.isBefore(repeatDate, system.date.now()):\n\t\t\t \tupdateQ \u003d \"UPDATE participant.Events SET repeatDateUntil \u003d CURRENT_TIMESTAMP WHERE id \u003d ?\"\n\t\t\t\tsystem.db.runPrepUpdate(updateQ, [selectedEventId])\n\t\t\t\t\n\tsystem.perspective.closeDock(\u0027AddEventToSchedule\u0027)\n\t"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
