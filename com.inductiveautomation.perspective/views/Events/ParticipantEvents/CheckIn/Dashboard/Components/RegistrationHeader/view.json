{
  "custom": {},
  "params": {
    "text": "Winter Shelter Registration"
  },
  "propConfig": {
    "params.text": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 62
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "62px",
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.text"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "fontSize": "25px",
            "fontWeight": "lighter",
            "paddingLeft": "30px"
          }
        },
        "type": "ia.display.label"
      },
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "script": "\t\n\tsystem.perspective.closeDock(\u0027AddCongregationToSchedule\u0027)"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Icon"
        },
        "position": {
          "basis": "30px",
          "shrink": 0
        },
        "props": {
          "path": "material/close",
          "style": {
            "marginRight": "50px"
          }
        },
        "type": "ia.display.icon"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "background": "linear-gradient(90deg, rgba(22,165,189,1) 0%, rgba(13,185,165,1) 100%)"
      }
    },
    "type": "ia.container.flex"
  }
}