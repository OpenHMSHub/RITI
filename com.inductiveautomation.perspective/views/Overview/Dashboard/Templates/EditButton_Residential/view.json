{
  "custom": {},
  "params": {
    "rowData": {
      "edit": {
        "value": true
      },
      "id": {
        "value": -1
      }
    },
    "sectionName": "value",
    "selectedFiscalYear": "value"
  },
  "propConfig": {
    "params.rowData": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.sectionName": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.selectedFiscalYear": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
