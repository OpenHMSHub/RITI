{
  "custom": {
    "Data": [
      {
        "note": "New Note 2"
      }
    ]
  },
  "params": {
    "Date": "Apr. 19, 2020 at 11:32 AM",
    "ID": 1,
    "Size": 100
  },
  "propConfig": {
    "custom.Data": {
      "binding": {
        "config": {
          "parameters": {
            "NoteID": "{view.params.ID}"
          },
          "queryPath": "Vendors/SelectSingleNote",
          "returnFormat": "json"
        },
        "transforms": [
          {
            "code": "\tif len(value):\n\t\treturn value\n\telse:\n\t\treturn [{\n\t  \"Note\": \"\"\n\t}]",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "params.Date": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.ID": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.Size": {
      "binding": {
        "config": {
          "path": "view.props.defaultSize.height"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 100
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "EmbeddedView"
        },
        "position": {
          "basis": "40px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.Date": {
            "binding": {
              "config": {
                "expression": "{view.params.Date}"
              },
              "transforms": [
                {
                  "expression": "dateFormat({value},\"MMM. d, YYYY\") +\" at \"+ dateFormat({value},\"hh:mm a\")",
                  "type": "expression"
                }
              ],
              "type": "expr"
            }
          }
        },
        "props": {
          "params": {
            "Icon": "material/description",
            "Title": "Note"
          },
          "path": "Vendors/Activity/ActivityHeader"
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "EmbeddedView_3"
        },
        "position": {
          "basis": "45px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.Text": {
            "binding": {
              "config": {
                "path": "view.custom.Data[0].note"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "path": "Vendors/Activity/Note/Comment",
          "style": {
            "borderTopColor": "#e6e6e6",
            "borderTopStyle": "solid",
            "borderTopWidth": 1,
            "marginLeft": "50px",
            "marginRight": "50px",
            "paddingTop": "5px"
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Background/Default",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}