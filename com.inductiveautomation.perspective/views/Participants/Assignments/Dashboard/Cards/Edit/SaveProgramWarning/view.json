{
  "custom": {
    "data": [],
    "function": 0,
    "suspendedNamesString": ""
  },
  "params": {
    "allParticipantsList": [],
    "employee_id": "value",
    "enrollment_id": "value",
    "suspendedParticipantNames": [],
    "time_created": "value",
    "user_name": "value"
  },
  "propConfig": {
    "custom.data": {
      "persistent": true
    },
    "custom.function": {
      "persistent": true
    },
    "custom.suspendedNamesString": {
      "binding": {
        "config": {
          "path": "view.params.suspendedParticipantNames"
        },
        "transforms": [
          {
            "code": "\tif value is not None and len(value) \u003e 0:\n\t\treturn \",\".join(value)\n\telse:\n\t\treturn \"\"",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "params.allParticipantsList": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.employee_id": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.enrollment_id": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.suspendedParticipantNames": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.time_created": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.user_name": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 190,
      "width": 300
    },
    "loading": {
      "mode": "blocking"
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "EmbeddedView Header"
        },
        "position": {
          "basis": "25px"
        },
        "props": {
          "params": {
            "Text": "Warning"
          },
          "path": "Shelter/Reservation/Cards/New Reservation/ReservationHeader"
        },
        "type": "ia.display.view"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "basis": "50px",
              "grow": 1
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "expression": "concat(\"Participant(s) - \" , {view.custom.suspendedNamesString} , \" are suspended. Do you want to proceed with the program creation for them ?\")"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "style": {
                "classes": "Text/Error_Message"
              }
            },
            "type": "ia.display.label"
          }
        ],
        "custom": {
          "ReservationSaved": false
        },
        "meta": {
          "name": "Message"
        },
        "position": {
          "basis": "90px",
          "shrink": 0
        },
        "props": {
          "style": {
            "borderTopColor": "#9db8d1",
            "borderTopStyle": "solid",
            "borderTopWidth": "1px",
            "classes": "Background/Default",
            "marginLeft": "10px",
            "marginRight": "10px",
            "marginTop": "5px",
            "paddingTop": "5px"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "script": "\tpayload \u003d {\"allow_suspended\":True}\n\tsystem.perspective.sendMessage(\"suspendedParticipantsResponse\",payload\u003dpayload, scope\u003d\"session\")\n\tsystem.perspective.closePopup(\"BatchSaveProgramWarning\")"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Yes"
            },
            "position": {
              "basis": "100px"
            },
            "props": {
              "style": {
                "classes": "Buttons/Main",
                "marginBottom": "5px",
                "marginLeft": "5px",
                "marginTop": "5px"
              },
              "text": "Yes"
            },
            "type": "ia.input.button"
          },
          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "script": "\tpayload \u003d {\"allow_suspended\":False}\n\tsystem.perspective.sendMessage(\"suspendedParticipantsResponse\",payload\u003dpayload, scope\u003d\"session\")\n\tsystem.perspective.closePopup(\"BatchSaveProgramWarning\")"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "No"
            },
            "position": {
              "basis": "100px"
            },
            "props": {
              "style": {
                "classes": "Buttons/Secondary",
                "margin": "5px"
              },
              "text": "No"
            },
            "type": "ia.input.button"
          }
        ],
        "custom": {
          "ReservationSaved": false
        },
        "meta": {
          "name": "root Actions"
        },
        "position": {
          "basis": "51px"
        },
        "props": {
          "justify": "center",
          "style": {
            "borderTopColor": "#9db8d1",
            "borderTopStyle": "solid",
            "borderTopWidth": "1px",
            "classes": "Background/Default",
            "marginLeft": "10px",
            "marginRight": "10px",
            "marginTop": "5px",
            "paddingTop": "5px"
          }
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Background/Default",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}