{
  "custom": {},
  "params": {
    "label": "label",
    "placeholder": "placeholder",
    "value": 0
  },
  "propConfig": {
    "params.label": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.placeholder": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 60
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "basis": "25px",
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.label"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Text/Add_HeaderText"
          },
          "textStyle": {
            "fontWeight": "bold"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "NumericEntryField"
        },
        "position": {
          "basis": "25px",
          "shrink": 0
        },
        "propConfig": {
          "props.placeholder": {
            "binding": {
              "config": {
                "path": "view.params.placeholder"
              },
              "type": "property"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.value"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "align": "left",
          "containerStyle": {
            "marginLeft": "10px",
            "marginTop": "10px"
          },
          "format": "0,0",
          "inputBounds": {
            "minimum": 1
          },
          "style": {
            "classes": "Text/Add_Value",
            "color": "#0d80e3",
            "fontSize": "15px"
          }
        },
        "type": "ia.input.numeric-entry-field"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "overflow": "visible"
      }
    },
    "type": "ia.container.flex"
  }
}