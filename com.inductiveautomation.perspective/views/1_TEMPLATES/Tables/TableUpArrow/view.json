{
  "custom": {},
  "params": {
    "row": "value",
    "rowIndex": "value",
    "value": "value"
  },
  "propConfig": {
    "params.row": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.rowIndex": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 19,
      "width": 44
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Icon"
        },
        "position": {
          "basis": "24px",
          "grow": 1
        },
        "propConfig": {
          "props.path": {
            "binding": {
              "config": {
                "expression": "if ({view.params.value} !\u003d -1, \u0027material/keyboard_arrow_up\u0027,\u0027\u0027)"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "color": "#0C4E85"
        },
        "type": "ia.display.icon"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}