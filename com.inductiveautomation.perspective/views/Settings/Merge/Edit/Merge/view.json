    "params.Dataset": {
      "binding": {
        "config": {
          "path": "/root/MergeTable.props.data"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    },
    "params.MergeParticipants": {
      "onChange": {
        "enabled": null,
        "script": "\toriginalData \u003d self.getChild(\"root\").getChild(\"MergeTable\").props.data\n\tif currentValue.value !\u003d None and currentValue.value !\u003d \u0027\u0027 and len(currentValue.value)\u003e0:\n#\t\tif len(originalData)\u003d\u003d1 and (self.getChild(\"root\").getChild(\"MergeTable\").props.data[0].participant_id \u003d\u003d None or self.getChild(\"root\").getChild(\"MergeTable\").props.data[0].participant_id \u003d\u003d \u0027\u0027):\n#\t\t\tself.getChild(\"root\").getChild(\"MergeTable\").props.data \u003d currentValue.value[0]\n#\t\telse:\n\t\tfor i in range(0,len(currentValue.value)):\n\t\t\toriginalData.append(currentValue.value[i])\n\tsystem.perspective.sendMessage(\"RemoveFromHuman\", payload \u003d {\u0027selected_dataset\u0027:self.getChild(\"root\").getChild(\"MergeTable\").props.data}, scope \u003d \"page\")"
      },
      "paramDirection": "input",
      "persistent": true
    },
    "params.SelectedParticipantToKeep": {
      "binding": {
        "config": {
          "path": "/root/MergeTable.props.selection.data"
        },
        "type": "property"
      },
      "paramDirection": "output",
      "persistent": true
    }
  },
